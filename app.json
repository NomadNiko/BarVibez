{
  "expo": {
    "name": "Bar Vibez",
    "slug": "barvibez",
    "version": "1.0.5",
    "scheme": "barvibez",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "us.nomadsoft.barvibez",
      "icon": "./assets/icon-ios.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": []
      }
    },
    "android": {
      "icon": "./assets/icon-android.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "us.nomadsoft.barvibez",
      "permissions": ["com.android.vending.BILLING"]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "00d16e31-dd24-4e42-813b-0aaea27afd1f"
      },
      "revenueCat": {
        "iosApiKey": "appl_haaWKmZRYbSWNGZvqoXQPdwcRFb",
        "entitlementIdentifier": "Pro"
      }
    }
  }
}
